@code {

    protected ElementReference element;

    private string _id = null;
    protected string Id
    {
        get
        {
            if (_id == null)
            {
                _id = GenerateId();
            }

            return _id;
        }
    }

    protected string GenerateId()
    {
        return Guid.NewGuid().ToString();
    }
}
